{
  "name": "ardemedia",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "run-p \"dev -- --open\" start:*",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "tailwind": "npx tailwindcss -i ./src/app.css -o ./src/theme/tailwind.css --watch",
    "start:firebase": "firebase emulators:start --import=./firestore-mock-data --export-on-exit",
    "start:functions": "npm run watch --prefix functions"
  },
  "devDependencies": {
    "@beyonk/svelte-google-analytics": "^2.2.0",
    "@sveltejs/kit": "1.0.0-next.180",
    "@tailwindcss/typography": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.44.0",
    "svelte-adapter-firebase": "^0.13.1",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.0.9",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "type": "module",
  "dependencies": {
    "clsx": "^1.1.1",
    "daisyui": "^1.21.0",
    "firebase": "^9.6.5",
    "svelte-icons": "^2.1.0"
  }
}
